{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "700px",
            "wideSize": "1500px",
            "allowCustomContentAndWideSize": true,
            "allowEditing": true
        },
        "color": {
            "custom": true,
            "customDuotone": true,
            "customGradient": true,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [
            ],
            "gradients": [
                {
                    "gradient": "linear-gradient(135deg, #e5e7eb 0%, #9ca3af  100%)",
                    "name": "グレイグラデーション",
                    "slug": "gray-gradient"
                },
                {
                    "gradient": "linear-gradient(135deg, #f0f9ff 0%, #7dd3fc 100%)",
                    "name": "スカイグラデーション",
                    "slug": "sky-gradient"
                }
            ],
            "palette": [
                {
                    "name": "Primary",
                    "slug": "primary",
                    "color": "#0ea5e9"
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "color": "#f97316"
                },
                {
                    "name": "Tertiary",
                    "slug": "tertiary",
                    "color": "#8b5cf6"
                },
                {
                    "name": "Accent",
                    "slug": "accent",
                    "color": "#10b981"
                },
                {
                    "name": "Primary Dark",
                    "slug": "primary-dark",
                    "color": "#082f49"
                },
                {
                    "name": "Light Gray",
                    "slug": "light-gray",
                    "color": "#f1f5f9"
                },
                {
                    "name": "Text Main",
                    "slug": "text-main",
                    "color": "#0f172a"
                },
                {
                    "name": "Text Sub",
                    "slug": "text-sub",
                    "color": "#cbd5e1"
                },
                {
                    "name": "Back Light",
                    "slug": "back-light",
                    "color": "#f0f9ff"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#FFFFFF"
                },
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#000000"
                },
                {
                    "name": "Yellow",
                    "slug": "yellow",
                    "color": "#eab308"
                },
                {
                    "name": "Red",
                    "slug": "red",
                    "color": "#ef4444"
                }
            ]
        },
        "typography": {
            "customFontSize": true,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textDecoration": true,
            "textTransform": true,
            "defaultFontSizes": false,
            "dropCap": false,
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "システム",
                    "slug": "system-font"
                },
                {
                    "fontFamily": "'Hiragino Kaku Gothic ProN', 'Hiragino Sans', Meiryo, sans-serif",
                    "name": "日本語",
                    "slug": "japanese"
                },
                {
                    "fontFamily": "Georgia, 'Times New Roman', Times, serif",
                    "name": "英字セリフ",
                    "slug": "serif"
                }
            ],
            "fontSizes": [
                {
                    "fluid": false,
                    "name": "XSmall",
                    "size": "0.75rem",
                    "slug": "x-small"
                },
                {
                    "fluid": false,
                    "name": "Small",
                    "size": "0.875rem",
                    "slug": "small"
                },
                {
                    "fluid": false,
                    "name": "Medium",
                    "size": "1rem",
                    "slug": "medium"
                },
                {
                    "fluid": false,
                    "name": "Large",
                    "size": "1.125rem",
                    "slug": "large"
                },
                {
                    "fluid": false,
                    "name": "XLarge",
                    "size": "1.25rem",
                    "slug": "x-large"
                },
                {
                    "fluid": false,
                    "name": "2xLarge",
                    "size": "1.5rem",
                    "slug": "2x-large"
                },
                {
                    "fluid": false,
                    "name": "3XLarge",
                    "size": "1.875rem",
                    "slug": "3x-large"
                },
                {
                    "fluid": false,
                    "name": "4XLarge",
                    "size": "2rem",
                    "slug": "4x-large"
                },
                {
                    "fluid": {
                        "max": "3rem",
                        "min": "2.15rem"
                    },
                    "name": "5XLarge",
                    "size": "3rem",
                    "slug": "5x-large"
                }
            ]
        },
        "spacing": {
            "customSpacingSize": true,
            "spacingSizes": [
                {
                    "name": "0",
                    "size": "0px",
                    "slug": "0"
                },
                {
                    "name": "5",
                    "size": "clamp(5px, 1vw, 5px)",
                    "slug": "5"
                },
                {
                    "name": "10",
                    "size": "clamp(10px, 1.5vw, 10px)",
                    "slug": "10"
                },
                {
                    "name": "20",
                    "size": "clamp(15px, 2vw, 20px)",
                    "slug": "20"
                },
                {
                    "name": "30",
                    "size": "clamp(25px, 3vw, 30px)",
                    "slug": "30"
                },
                {
                    "name": "40",
                    "size": "clamp(30px, 4vw, 40px)",
                    "slug": "40"
                },
                {
                    "name": "50",
                    "size": "clamp(35px, 5vw, 50px)",
                    "slug": "50"
                },
                {
                    "name": "60",
                    "size": "clamp(40px, 6vw, 60px)",
                    "slug": "60"
                },
                {
                    "name": "70",
                    "size": "clamp(50px, 7vw, 70px)",
                    "slug": "70"
                },
                {
                    "name": "80",
                    "size": "clamp(60px, 8vw, 80px)",
                    "slug": "80"
                }
            ],
            "units": ["px", "em", "rem", "vh", "vw", "%"]
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        }
    },
    "styles": {
        "blocks": {
            "core/navigation": {
                "spacing": {
                    "blockGap": "var(--wp--preset--spacing--20)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                },
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--primary)"
                        },
                        ":hover": {
                            "color": {
                                "text": "var(--wp--preset--color--tertiary)"
                            }
                        }
                    }
                }
            },
            "core/site-title": {
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--text-main)"
                        },
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--5-x-large)",
                    "fontWeight": "700",
                    "lineHeight": "1.25"
                }
            },
            "core/post-title": {
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--text-tertiary)"
                        },
                        ":hover": {
                            "color": {
                                "text": "var(--wp--preset--color--primary)"
                            }
                        }
                    }
                },
                "typography": {
                    "fontSize": "2.5rem",
                    "fontWeight": "700",
                    "lineHeight": "1.25"
                }
            },
            "core/group": {
                "spacing": {
                    "padding": {
                        "top": "0",
                        "bottom": "0"
                    }
                }
            }
        },
        "color": {
            "text": "var(--wp--preset--color--text-main)",
            "background": "var(--wp--preset--color--backlight)"
        },
        "elements": {
            "button": {
                "border": {
                    "radius": "0.5rem"
                },
                "color": {
                    "background": "var(--wp--preset--color--primary-dark)",
                    "text": "var(--wp--preset--color--white)"
                },
                "spacing": {
                    "padding": {
                        "top": "0.625rem",
                        "right": "1.25rem",
                        "bottom": "0.625rem",
                        "left": "1.25rem"
                    }
                },
                "typography": {
                    "fontWeight": "600",
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "1.4"
                },
                ":hover": {
                    "color": {
                        "background": "var(--wp--preset--color--primary)",
                        "text": "var(--wp--preset--color--white)"
                    }
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4x-large)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3x-large)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2x-large)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "heading": {
                "typography": {
                    "fontWeight": "600",
                    "lineHeight": "1.25"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--tertiary)"
                    }
                }
            }
        },
        "spacing": {
            "blockGap": "0",
            "padding": {
                "top": "0",
                "right": "0",
                "bottom": "0",
                "left": "0"
            },
            "margin": {
                "top": "0",
                "right": "0",
                "bottom": "0",
                "left": "0"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font--family--japanese)",
            "fontSize": "var(--wp--preset--font-size--medium)",
            "lineHeight": "1.6"
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "ヘッダー",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "フッター",
            "area": "footer"
        },
        {
            "name": "sidebar",
            "title": "サイドバー",
            "area": "sidebar"
        }
    ]
}
